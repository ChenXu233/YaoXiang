//! # fibonacci_iterative - 迭代斐波那契
//!
//! 测试整数运算和循环性能

use std.io

fibonacci: (n: Int) -> Int = {
    if n <= 1 {
        return n
    } else {
        mut a = 0;
        mut b = 1;
        mut result = 0;
        for i in 2..(n + 1) {
            result = a + b;
            a = b;
            b = result;
        }
        return result
    }
};

// 主函数
main: () -> Int = {
    print(fibonacci(10))
}
