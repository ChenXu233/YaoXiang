// 方法绑定功能测试
type Point = {
    x: Float,
    y: Float
}

Point.distance: (Point,Point) -> Float = (p1, p2) => {
    dx = p2.x - p1.x
    dy = p2.y - p1.y
    return (dx * dx + dy * dy)**0.5
}

main: () -> Void = () => {
    p1: Point = Point(1.0, 2.0)
    p2: Point = Point(3.0, 4.0)
    d: Float = p1.distance(p2)
    return
}
