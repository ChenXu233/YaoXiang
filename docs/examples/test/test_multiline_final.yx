// 测试多行字符串功能
use std.io

main = () => {
    // 测试基本的多行字符串
    json = """
    {
        "name": "张三",
        "age": 30
    }
    """
    println(json)

    // 测试空的多行字符串
    empty = """"""
    println("Empty string length: " + empty.length().to_string())

    // 测试包含引号的多行字符串
    quote_test = """这是"引号"测试"""
    println(quote_test)

    println("\n=== 多行字符串测试成功！ ===")
}
